@model DetalhesAutomovelViewModel
<div class="row gap-3 my-3 my-xl-5">
    <label class="fs-4"><strong>Modelo:</strong> @Model.Modelo</label>

    <label class="fs-5"><strong>Id:</strong> [@Model.Id]</label>
    <label class="fs-5"><strong>Marca:</strong> @Model.Marca</label>
    <label class="fs-5"><strong>Cor:</strong> @Model.Cor</label>
    <label class="fs-5"><strong>Placa:</strong> @Model.Placa</label>
    <label class="fs-5"><strong>Combustível:</strong> @Model.Combustivel</label>
    <label class="fs-5"><strong>Ano:</strong> @Model.Ano</label>
    <label class="fs-5"><strong>Capacidade de Combustível:</strong> @Model.CapacidadeCombustivel</label>
    <label class="fs-5"><strong>Foto do Veículo:</strong> @Model.FotoVeiculo</label>
</div>
